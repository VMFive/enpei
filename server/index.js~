// Generated by LiveScript 1.4.0
(function(){
  var fs, config, app, env, port, domainSocket;
  fs = require('fs');
  config = require('./config');
  app = require('./app');
  env = process.env;
  if (env.PORT) {
    port = env.PORT;
    app.listen(port);
    console.log("Listening on port " + port);
  } else {
    domainSocket = config.domainSocket;
    app.listen(domainSocket);
    fs.chmod(domainSocket, '777');
    console.log("Listening on domain socket " + domainSocket);
  }
}).call(this);
